{
  "name": "bs-graphql-schema-parser",
  "namespace": true,
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "generators": [
        {
          "name": "menhir",
          "edge": ["parser.ml", "parser.mli", ":", "parser.mly"]
        },
        {
          "name": "ocamllex",
          "edge": ["lexer.ml", ":", "lexer.mll"]
        }
      ]
    }
  ],
  "generators": [
    { "name": "menhir", "command": "menhir $in" },
    { "name": "ocamllex", "command": "ocamllex $in" }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3
}
