{
  "name": "bs-graphql-json-parser",
  "namespace": true,
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "generators": [
        {
          "name": "menhir",
          "edge": [
            "menhir_parser.ml",
            "menhir_parser.mli",
            ":",
            "menhir_parser.mly"
          ]
        },
        {
          "name": "ocamllex",
          "edge": ["ocamllex_lexer.ml", ":", "ocamllex_lexer.mll"]
        }
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "generators": [
    { "name": "menhir", "command": "menhir $in" },
    { "name": "ocamllex", "command": "ocamllex $in" }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": false
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3
}
